{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
{
  "rewrites": [
    { "source": "/reset.html", "destination": "/reset.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
